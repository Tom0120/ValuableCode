<?xml version="1.0" encoding="utf-8"?><Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">  <PropertyGroup>    <SolutionDir>$(MSBuildProjectDirectory)\..\..\</SolutionDir>    <BuildProperties>Platform=AnyCPU;Configuration=$(Configuration);SolutionDir=$(SolutionDir);</BuildProperties>    <IsDesignBuild>False</IsDesignBuild>    <TargetDir>$(OutDir)</TargetDir>  </PropertyGroup>  <Target Name="Build">    <MSBuild Projects="$(SolutionDir)SemTools.Web\SemTools.Web.csproj" Properties="$(BuildProperties)OutDir=$(OutDir)Temp\\;" />    <MSBuild Projects="$(SolutionDir)DBBackup\DBBackup.csproj" Properties="$(BuildProperties)OutDir=$(OutDir)DBBackup\\;" />    <MSBuild Projects="$(SolutionDir)SemTools.DataCreator\SemTools.DataCreator.csproj" Properties="$(BuildProperties)OutDir=$(OutDir)DataCreator\\;" />    <MSBuild Projects="$(SolutionDir)SemTools.RestartIISService\SemTools.RestartIISService.csproj" Properties="$(BuildProperties)OutDir=$(OutDir)RestartIISService\\;" />    <MSBuild Projects="$(SolutionDir)GenerateMonthlyPerformance\GenerateMonthlyPerformance.csproj" Properties="$(BuildProperties)OutDir=$(OutDir)GenerateMonthlyPerformance\\;" />    <MSBuild Projects="$(SolutionDir)SemTools.Instrumentation.Database\SemTools.Instrumentation.Database.sqlproj" Properties="$(BuildProperties)OutDir=$(OutDir)SemTools.Instrumentation.Database\\;" />    <Exec Command="XCOPY /Y /E &quot;$(OutDir)Temp\_PublishedWebsites\SemTools.Web\*&quot; &quot;$(OutDir)SEMToolWeb\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\APAutoIIS\*&quot; &quot;$(OutDir)APAutoIIS\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\AggregatorService\*&quot; &quot;$(OutDir)AggregatorService\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\DataNodeService\*&quot; &quot;$(OutDir)DataNodeService\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\ZooKeeper\*&quot; &quot;$(OutDir)ZooKeeper\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\APLogsUploader\*&quot; &quot;$(OutDir)APLogsUploader\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\SemToolAutoSQLService\*&quot; &quot;$(OutDir)SemToolAutoSQLService\&quot;" />    <Exec Command="XCOPY /Y &quot;$(SolutionDir)SemTools.Deployment\SemToolServiceMap.ini&quot; &quot;$(OutDir)&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.Deployment\APAutoIIS\rewrite_amd64_en-US.msi&quot; &quot;$(OutDir)SEMToolWeb\&quot;" />    <Exec Command="XCOPY /Y /E &quot;$(SolutionDir)SemTools.DataCreator\bin\$(Configuration)\WordBreakerResources\*&quot; &quot;$(OutDir)DataCreator\WordBreakerResources\&quot;" />    <ItemGroup>      <TempFiles Include="$(OutDir)Temp\*.*"/>    </ItemGroup>    <Delete Files="@(TempFiles)" />    <RemoveDir Directories="$(OutDir)Temp\\" />  </Target>  <Target Name="Clean">    <ItemGroup>      <OriginalFiles Include="$(OutDir)\**\*.*"/>      <OriginalFolder Include="$([System.IO.Directory]::GetDirectories(&quot;$(OutDir)&quot;))" />    </ItemGroup>    <Delete Files="@(OriginalFiles)"  />    <Message Text="@(OriginalFolder)" />    <Message Text="@(OriginalFiles)" />    <RemoveDir Directories="@(OriginalFolder)" />  </Target></Project>